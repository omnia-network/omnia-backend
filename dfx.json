{
  "canisters": {
    "omnia_backend": {
      "candid": "src/omnia_backend/omnia_backend.did",
      "build": [
        "cargo build --target wasm32-unknown-unknown --release -p omnia_backend --locked",
        "gzip -f -k ./target/wasm32-unknown-unknown/release/omnia_backend.wasm"
      ],
      "wasm": "target/wasm32-unknown-unknown/release/omnia_backend.wasm.gz",
      "type": "custom",
      "dependencies": [
        "database"
      ]
    },
    "database": {
      "candid": "src/database/database.did",
      "package": "database",
      "type": "rust"
    },
    "ledger": {
      "type": "custom",
      "wasm": "https://download.dfinity.systems/ic/5eb2810653d4c72c7afc48a0450e2ad01378dfc7/canisters/ledger-canister_notify-method.wasm.gz",
      "candid": "icp-ledger/ledger.public.did",
      "remote": {
        "candid": "icp-ledger/ledger.public.did",
        "id": {
          "ic": "ryjl3-tyaaa-aaaaa-aaaba-cai"
        }
      }
    },
    "application_placeholder": {
      "candid": "src/application_placeholder/application_placeholder.did",
      "package": "application_placeholder",
      "type": "rust",
      "dependencies": [
        "database",
        "omnia_backend"
      ],
      "__0": "Here, the remote canister id of the ledger is only used to avoid deploying the application_placeholder canister to the IC.",
      "remote": {
        "id": {
          "ic": "ryjl3-tyaaa-aaaaa-aaaba-cai"
        }
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    },
    "replica": {
      "subnet_type": "system"
    }
  },
  "dfx": "0.14.0",
  "output_env_file": ".env",
  "version": 1
}
